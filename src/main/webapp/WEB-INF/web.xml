<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
   http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">    
    
    <servlet>
        <servlet-name>ServPlaceExist</servlet-name>
        <servlet-class>createcamp.inclusivityreview.ServletPlaceExist</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServPlaceExist</servlet-name>
        <url-pattern>/exist</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>ServPlaceAdd</servlet-name>
        <servlet-class>createcamp.inclusivityreview.ServletPlaceAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServPlaceAdd</servlet-name>
        <url-pattern>/addplace</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServGetReviews</servlet-name>
        <servlet-class>createcamp.inclusivityreview.ServletGetReviews</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServGetReviews</servlet-name>
        <url-pattern>/review</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServAddReview</servlet-name>
        <servlet-class>createcamp.inclusivityreview.ServletAddReview</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServAddReview</servlet-name>
        <url-pattern>/addreview</url-pattern>
    </servlet-mapping>

</web-app>